<div id="conteiner">
<h1>Compras Ap 212</h1>

<link href="/assets/seu-arquivo.css" rel="stylesheet">

<h2>Quarto</h2>

<ul>
      <% @quartos.each do |quarto| %>

      <div id="item_conteiner">  
      <li><%= quarto.item %></li>  
      </div>
        <%= button_to quarto, method: :delete, class: "delete-button" do %>
            <img src="data:image/png;base64,"/>
          <% end %>
         
        <% end %>

    <button id="btnnovo" type="button" onclick="window.location.href='<%= new_quarto_path %>'">
      <img src="data:image/png;base64," alt="Adicionar">
    </button>
</ul>



